{
    "multipart": [
        {   "when": { "facing": "north", "sliding": "false" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door"}
        },
        {   "when": { "facing": "south", "sliding": "false" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door", "y": 180}
        },
        {   "when": { "facing": "west", "sliding": "false" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door", "y": 270}
        },
        {   "when": { "facing": "east", "sliding": "false" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door", "y": 90}
        },
        {   "when": { "facing": "north", "holder": "true" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door_bar"}
        },
        {   "when": { "facing": "south", "holder": "true" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door_bar", "y": 180}
        },
        {   "when": { "facing": "west", "holder": "true" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door_bar", "y": 270}
        },
        {   "when": { "facing": "east", "holder": "true" },
            "apply": { "model": "rafradek_tf2_weapons:overhead_door_bar", "y": 90}
        }
    ]
}
